          "#[var cs    = $(csVersion.Replace(\"%Version%\", \"$(num0Bridge.Replace('.', ', ')), $(revBuild)\").Replace(\"%namespace%\", \".Bridge\"))]",
          "#[var cs    = $(cs.Replace(\"%VersionRev%\", \"$(revBuild)\").Replace(\"%VersionString%\", \"$(num0Bridge)\"))]",
